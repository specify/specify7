    <viewdefs>
        <viewdef
            type="form"
            name="label"
            class="edu.ku.brc.ui.forms.persist.FormCell"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            <desc><![CDATA[The label form.]]></desc>
            <enableRules/>

            <columnDef>140px,2px,f:p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2dlu"/>
            
            <rows>
                <row>
                	<cell type="subview" viewname="baseFormCell" id="1" name="" colspan="3"/>
                </row> 
                <row>
                    <cell type="label" labelfor="2" label="Label"/>
                    <cell type="field" uitype="text" id="2" name="label" isrequired="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="3" label="Label For"/>
                    <cell type="field" uitype="combobox" id="3" name="labelForCBX" isrequired="false"/>
                </row>
                <row>
