                </row>
                 <row>
                    <cell type="label" labelfor="5" label="Is Record Obj"/>
                    <cell type="field" uitype="checkbox" id="5" name="isRecordObj" isrequired="false"/>
                </row>
            </rows>
        </viewdef>
        
