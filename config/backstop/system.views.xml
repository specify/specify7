        <viewdef
            type="form"
            name="User Group"
            class="edu.ku.brc.specify.datamodel.SpPrincipal"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            
            <desc><![CDATA[User Group Data Entry Form]]></desc>
            <columnDef>p,3dlu,p,p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2dlu"/>            

            <rows>
                <row>
                    <cell type="separator" label="User Group" colspan="4"/>
                </row>
                <row>
                    <cell type="label" labelfor="1" label="Name"/>
                    <cell type="field" id="1" name="name" uitype="text" cols="40" isrequired="true"/> 
                </row>
                <row>
                    <cell type="label" labelfor="2" label="Remarks"/>
                    <cell type="field" id="2" name="remarks" uitype="textareabrief" cols="40" rows="2" colspan="2"/>
                </row>
            </rows>
        </viewdef>

        <viewdef
            type="form"
            name="User"
            class="edu.ku.brc.specify.datamodel.SpecifyUser"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            
            <desc><![CDATA[User Data Entry Form]]></desc>
            <columnDef>p,3dlu,p,6px,p,6px,p,p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2dlu"/>            

            <rows>
                <row>
                    <cell type="separator" label="User" colspan="8"/>
                </row>
                <row>
                    <cell type="label" labelfor="5"/>
                    <cell type="field" id="5" name="userType" uitype="dsptextfield" readonly="true"/> 
                </row>
                <row>
                    <cell type="label" labelfor="1"/>
                    <cell type="field" id="1" name="name" uitype="text" cols="40" isrequired="true" initialize="editoncreate=true"/> 
                </row>
                <row>
                    <cell type="label" labelfor="3"/>
                    <cell type="field" id="3" name="password" uitype="password" cols="15"/> 
                    <cell type="command" id="ShowPwd" name="ShowPwd" label="Show Password" ignore="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="6" label="Password Strength"/>
                    <cell type="field" id="6" uitype="plugin" name="this" initialize="name=PasswordStrengthUI"/>
                </row>
                <row>
                    <cell type="label" labelfor="2"/>
                    <cell type="field" id="2" name="email" uitype="text" cols="40" isrequired="false"/> 
                </row>
                <row>
                    <cell type="label" labelfor="4" label="Agent"/>
                    <cell type="field" id="4" name="agent" uitype="textfieldinfo" initialize="name=AgentSSPU;title=Agent;" isrequired="false" ignore="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="key" label="Key"/>
                    <cell type="field" id="key" name="key" uitype="text" ignore="true" readonly="true"/> 
                    <cell type="command" id="GenerateKey" name="GenerateKey" label="Generate Master Key" ignore="true"/>
                    <cell type="command" id="CopyToCB" name="CopyToCB" label="" initialize="icon=ClipboardCopy;size=24;tooltip=CPY_TO_CB_TT" ignore="true"/>
                </row>
                <!--row>
                    <cell type="label" labelfor="4" label="Retype Password"/>
                    <cell type="field" id="4" name="password" uitype="text" cols="15"/>
                    </row-->
            </rows>
        </viewdef>

