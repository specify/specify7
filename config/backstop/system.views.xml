    <viewdefs>
        <viewdef
            type="form"
            name="PickList"
            class="edu.ku.brc.specify.datamodel.PickList"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            
            <desc><![CDATA[Form for editing an PickList]]></desc>
            <enableRules>
                <rule id="sizeLimit"><![CDATA[readOnly.isSelected() == false]]></rule>
                <rule id="RemoveItem"><![CDATA[items.getSelectedIndex() != -1]]></rule>
                <rule id="AddItem"><![CDATA[title.isNotEmpty() && value.isNotEmpty()]]></rule>
                <rule id="name"><![CDATA[dataObj.getCreated() == null]]></rule>
                <rule id="deletePL"><![CDATA[dataObj.getCreated() != null && processor.getNumUsed() == 0]]></rule>
            </enableRules>
            <columnDef>100px, 2px, p, p, p:g, f:p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2dlu"/>
            <rows>
                <row>
                    <cell type="separator" label="Pick List" colspan="3"/>
                </row>
                <row>
                    <cell type="label" labelfor="typesCBX" label="Type"/>
                    <cell type="field" id="typesCBX" name="typesCBX" uitype="combobox" ignore="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="name" label="Name"/>
                    <cell type="field" id="name" name="name" uitype="text" isrequired="true" cols="30" colspan="3" initialize="editoncreate=true" />
                </row>
                <row>
                    <cell type="label" labelfor="readOnly" label=""/>
                    <cell type="field" id="readOnly" name="readOnly" label="Read Only" uitype="checkbox"/>
                </row>
                <row>
                    <cell type="label" labelfor="tablesCBX" label="Table Name"/>
                    <cell type="field" id="tablesCBX" name="tablesCBX" uitype="combobox" ignore="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="formatterCBX" label="Formatter"/>
                    <cell type="field" id="formatterCBX" name="formatterCBX" uitype="combobox" ignore="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="fieldsCBX" label="Field Name"/>
                    <cell type="field" id="fieldsCBX" name="fieldsCBX" uitype="combobox" ignore="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="sizeLimit" label="Size Limit"/>
                    <cell type="field" id="sizeLimit" name="sizeLimit" uitype="spinner" initialize="max=500;min=2" isrequired="true"/>
                </row>
                <row>
					<cell type="subview" viewname="PickListItem" id="5" name="pickListItems" colspan="6"/>
                </row>
          </rows>
        </viewdef>
        
        <viewdef
            type="formtable"
            name="PickListItem Table"
            class="edu.ku.brc.specify.datamodel.PickListItem"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            
            <desc><![CDATA[PickListItem grid view for PickList form.]]></desc>
            <definition>PickListItem</definition>
        </viewdef>
        
        <viewdef
            type="form"
            name="PickListItem"
            class="edu.ku.brc.specify.datamodel.PickListItem"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForObj"
            settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
            
            <desc><![CDATA[PickListItem]]></desc>
            <enableRules/>
            <columnDef>100px, 2px, p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2dlu"/>
            <rows>
                <row>
                    <cell type="separator" label="Items" colspan="3"/>
                </row>
                <row>
                    <cell type="label" labelfor="title" label="Title"/>
                    <cell type="field" id="title" name="title" uitype="text" isrequired="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="value" label="value"/>
                    <cell type="field" id="value" name="value" label="Value" uitype="text" isrequired="true"/>
                </row>
            </rows>
        </viewdef>
        
        <viewdef
            type="form"
            name="SendMail"
            class="java.util.Hashtable"
            gettable="edu.ku.brc.af.ui.forms.DataGetterForHashMap"
            settable="edu.ku.brc.af.ui.forms.DataSetterForHashMap">
            
            <desc><![CDATA[Send Mail Email Form]]></desc>
            <columnDef>p,3dlu,p:g</columnDef>
            <rowDef auto="true" cell="p" sep="2dlu"/>
            <rows>
                <row>
                    <cell type="label" labelfor="1" label="From"/>
                    <cell type="field" id="1" name="from" uitype="dsptextfield"/> 
                </row>
                <row>
                    <cell type="label" labelfor="2" label="To"/>
                    <cell type="field" id="2" name="to" uitype="text" isrequired="true"/>
                </row>
                <row>
                    <cell type="label" labelfor="3" label="Subject"/>
                    <cell type="field" id="3" name="subject" uitype="text"/>
                </row>
                <row>
                    <cell type="separator" label="" colspan="3"/>
                </row>                
                <row>
                    <cell type="field" id="4" name="bodytext" uitype="textarea" cols="60" rows="10" colspan="3"/>
                </row>
                <row>
                    <cell type="label" labelfor="5" label="Attachment"/>
                    <cell type="field" id="5" name="attachedFileName" uitype="dsptextfield"/>
                </row>
            </rows>
        </viewdef>
        
        <viewdef
            type="form"
            name="PrepType"
