	<viewdefs>
		<viewdef name="PickList" class="edu.ku.brc.specify.datamodel.PickList" type="form" gettable="edu.ku.brc.af.ui.forms.DataGetterForObj" settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
			<desc>Form for editing an PickList</desc>
			<enableRules>
				<rule id="sizeLimit">readOnly.isSelected() == false</rule>
				<rule id="RemoveItem">items.getSelectedIndex() != -1</rule>
				<rule id="AddItem">title.isNotEmpty() &amp;&amp; value.isNotEmpty()</rule>
				<rule id="name">dataObj.getCreated() == null</rule>
				<rule id="deletePL">dataObj.getCreated() != null &amp;&amp; processor.getNumUsed() == 0</rule>
			</enableRules>
			<columnDef>80px,2px,200px,2px,75px,2px,p:g</columnDef>
			<rowDef auto="true" cell="p" sep="2dlu"/>
			<rows>
				<row>
					<cell type="separator" label="Pick List" colspan="7"/>
				</row>
				<row>
					<cell type="label" labelfor="typesCBX" label="Type"/>
					<cell type="field" id="typesCBX" name="typesCBX" uitype="combobox" ignore="true"/>
				</row>
				<row>
					<cell type="label" labelfor="name" label="Name"/>
					<cell type="field" id="name" name="name" uitype="text" isrequired="true" colspan="1" initialize="editoncreate=true"/>
					<cell type="field" id="readOnly" name="readOnly" label="Read Only" uitype="checkbox" initialize="align=left"/>
				</row>
				<row>
					<cell type="label" labelfor="tablesCBX"/>
					<cell type="field" id="tablesCBX" name="tablesCBX" uitype="combobox" ignore="true"/>
				</row>
				<row>
					<cell type="label" labelfor="formatterCBX" label="Formatter"/>
					<cell type="field" id="formatterCBX" name="formatterCBX" uitype="combobox" ignore="true"/>
				</row>
				<row>
					<cell type="label" labelfor="fieldsCBX"/>
					<cell type="field" id="fieldsCBX" name="fieldsCBX" uitype="combobox" ignore="true"/>
				</row>
				<row>
					<cell type="label" labelfor="sizeLimit"/>
					<cell type="field" id="sizeLimit" name="sizeLimit" uitype="spinner" initialize="min=-1" isrequired="true"/>
				</row>
				<row>
					<cell type="subview" viewname="PickListItem" id="5" name="pickListItems" colspan="6"/>
				</row>
			</rows>
		</viewdef>
		<viewdef type="formtable" name="PickListItem Table" class="edu.ku.brc.specify.datamodel.PickListItem" gettable="edu.ku.brc.af.ui.forms.DataGetterForObj" settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
			<desc>PickListItem grid view for PickList form.</desc>
			<definition>PickListItem</definition>
		</viewdef>
		<viewdef name="PickListItem" class="edu.ku.brc.specify.datamodel.PickListItem" type="form" gettable="edu.ku.brc.af.ui.forms.DataGetterForObj" settable="edu.ku.brc.af.ui.forms.DataSetterForObj">
			<desc>PickListItem</desc>
			<enableRules/>
			<columnDef>30px,2px,200px,p:g</columnDef>
			<rowDef auto="true" cell="p" sep="2dlu"/>
			<rows>
				<row>
					<cell type="label" labelfor="title"/>
					<cell type="field" id="title" name="title" uitype="text" isrequired="true"/>
				</row>
				<row>
					<cell type="label" labelfor="value"/>
					<cell type="field" id="value" name="value" label="Value" uitype="text" isrequired="true"/>
				</row>
			</rows>
		</viewdef>
		<viewdef type="form" name="SendMail" class="java.util.Hashtable" gettable="edu.ku.brc.af.ui.forms.DataGetterForHashMap" settable="edu.ku.brc.af.ui.forms.DataSetterForHashMap">
			<desc>Send Mail Email Form</desc>
			<columnDef>p,3dlu,p:g</columnDef>
			<rowDef auto="true" cell="p" sep="2dlu"/>
			<rows>
				<row>
					<cell type="label" labelfor="1" label="From"/>
					<cell type="field" id="1" name="from" uitype="dsptextfield"/>
				</row>
				<row>
					<cell type="label" labelfor="2" label="To"/>
					<cell type="field" id="2" name="to" uitype="text" isrequired="true"/>
				</row>
				<row>
					<cell type="label" labelfor="3" label="Subject"/>
					<cell type="field" id="3" name="subject" uitype="text"/>
				</row>
				<row>
					<cell type="separator" label="" colspan="3"/>
				</row>
				<row>
					<cell type="field" id="4" name="bodytext" uitype="textarea" cols="60" rows="10" colspan="3"/>
				</row>
				<row>
					<cell type="label" labelfor="5" label="Attachment"/>
					<cell type="field" id="5" name="attachedFileName" uitype="dsptextfield"/>
				</row>
			</rows>
		</viewdef>
