<?xml version="1.0" encoding="UTF-8"?>
<viewset name="Common" i18nresname="common" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <views>
        <view name="CollectionObjectType"
              class="edu.ku.brc.specify.datamodel.CollectionObjectType"
              busrules="edu.ku.brc.specify.datamodel.busrules.CollectionObjectTypeBusRules"
              resourcelabels="false">
            <desc><![CDATA[The Collection Object Type form.]]></desc>
            <altviews>
                <altview name="CollectionObjectType View" viewdef="CollectionObjectType" mode="view" default="true"/>
                <altview name="CollectionObjectType Edit" viewdef="CollectionObjectType" mode="edit"/>
            </altviews>
        </view>

        <view name="CollectionObjectGroup"
              class="edu.ku.brc.specify.datamodel.CollectionObjectGroup"
              busrules="edu.ku.brc.specify.datamodel.busrules.CollectionObjectGroupBusRules"
              resourcelabels="false">
            <desc><![CDATA[The Collection Object Type form.]]></desc>
            <altviews>
                <altview name="CollectionObjectGroup View" viewdef="CollectionObjectGroup" mode="view" default="true"/>
                <altview name="CollectionObjectGroup Edit" viewdef="CollectionObjectGroup" mode="edit"/>
            </altviews>
        </view>

        <view name="CollectionObjectGroupType"
              class="edu.ku.brc.specify.datamodel.CollectionObjectGroupType"
              busrules="edu.ku.brc.specify.datamodel.busrules.CollectionObjectGroupTypeBusRules"
              resourcelabels="false">
            <desc><![CDATA[The Collection Object Type form.]]></desc>
            <altviews>
                <altview name="CollectionObjectGroupType View" viewdef="CollectionObjectGroupType" mode="view" default="true"/>
                <altview name="CollectionObjectGroupType Edit" viewdef="CollectionObjectGroupType" mode="edit"/>
            </altviews>
        </view>

        <view name="CollectionObjectGroupJoin"
              class="edu.ku.brc.specify.datamodel.CollectionObjectGroupJoin"
              busrules="edu.ku.brc.specify.datamodel.busrules.CollectionObjectGroupJoinBusRules"
              resourcelabels="false">
            <desc><![CDATA[The Collection Object Type form.]]></desc>
            <altviews>
                <altview name="CollectionObjectGroupJoin View" viewdef="CollectionObjectGroupJoin" mode="view" default="true"/>
                <altview name="CollectionObjectGroupJoin Edit" viewdef="CollectionObjectGroupJoin" mode="edit"/>
            </altviews>
        </view>
    </views>

    <!--
